Extension { #name : #GtCoderExecutionContextVariable }

{ #category : #'*GToolkit-Notifications' }
GtCoderExecutionContextVariable class >> removeNotification: aNotification [
	self value removeNotification: aNotification
]

{ #category : #'*GToolkit-Notifications' }
GtCoderExecutionContextVariable class >> showNotification: aNotification [
	self value showNotification: aNotification
]
